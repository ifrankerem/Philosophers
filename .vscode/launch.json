{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Philo Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/philo",
      "args": [
        "4",
        "410",
        "200",
        "200"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
